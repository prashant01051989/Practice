package com.test.hibernate.persis;

import org.apache.log4j.Logger;

public class Customer {
	private static final Logger LOG = Logger.getLogger(Customer.class);
	private int cid;
	private String cname;
	private String email;
	private String city;
	private String status;
	private long phone;

	public Customer() {
		LOG.info("Customer 0 arg cons");
	}

	public Customer(int cid, String cname, String email, String city,
			String status, long phone) {
		super();
		LOG.info("Customer  arg cons");
		this.cid = cid;
		this.cname = cname;
		this.email = email;
		this.city = city;
		this.status = status;
		this.phone = phone;
	}

	
	
}
