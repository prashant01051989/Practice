import java.util.Arrays;


public class StirngTest {

	public static void main(String[] args) {
		String st="{\"name\":\"anshu\"}";
		String old=st.substring((st.indexOf('\"')+1), (st.indexOf(':')-1));
		st=st.replace(old, "Prashnat");
		System.out.println(st);
		String[] s=st.split("\"");
		s[1]="Anshu";
		old=Arrays.toString(s);
		System.out.println(old);

	}

}
