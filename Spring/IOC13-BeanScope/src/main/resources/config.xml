<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">


	<bean id="aObj" class="com.test.spring.ioc13.A" scope="prototype" lazy-init="false">
		<property name="a" value="10" />
		<property name="aName" value="AClass" />
	</bean>

	<bean id="bObj" class="com.test.spring.ioc13.B" scope="prototype">
		<property name="b" value="20" />
		<property name="bName" value="BClass" />
	</bean>


	<bean id="hello1" class="com.test.spring.ioc13.Hello" scope="singleton" lazy-init="true">
		<property name="a" ref="aObj" />
		<property name="b" ref="bObj" />
	</bean>

</beans>
