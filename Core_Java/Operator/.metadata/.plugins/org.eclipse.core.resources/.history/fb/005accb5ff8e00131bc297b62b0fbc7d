package com.test.controlStatement;

public class SwitchTest1 {

	public static void main(String[] args) {
		int a = 6;

		switch (a) {
		case 1:{
			System.out.println("1");
		}
		case 5:
			System.out.println("5");
		case 15:
			System.out.println("6");
			System.out.println("Hi");
		case 'a':
			System.out.println('a');
		}
	}

}
