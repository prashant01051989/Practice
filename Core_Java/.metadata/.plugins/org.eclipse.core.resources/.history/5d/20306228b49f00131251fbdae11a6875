package com.thread.test;

public class ThreadTest3 {

	public static void main(String[] args) {

	}

}


class HelloThread3{
	
	public void m1(){
		for(int i=0;i<5;i++){
			System.out.println("m1()- "+i+" by "+Thread.currentThread().getName());
			try
			{
				Thread.sleep(500);
			}catch(Exception e){}
		}
	}
	
	
	public void m2(){
		for(int i=0;i<5;i++){
		System.out.println("m1()- "+i+" by "+Thread.currentThread().getName());
		try
		{
			Thread.sleep(500);
		}cat
	}
	
}