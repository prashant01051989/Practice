package com.thread.test;

public class ThreadTest3 {

	public static void main(String[] args) {

	}

}

class HelloThread3 {

	public void m1() {
		for (int i = 0; i < 5; i++) {
			System.out.println("m1()- " + i + " by "
					+ Thread.currentThread().getName());
		}
	}

	public void m2() {
		for (int i = 0; i < 5; i++) {
			System.out.println("m1()- " + i + " by "
			+ Thread.currentThread().getName());
		}
	}

	public void m3() {
		for (int i = 12; i <= 17; i++) {
			System.out.println("m3()- " + i + " by "
					+ Thread.currentThread().getName());
		}
	}
}