package com.thread.test;

public class DeadLockTest {
	String st = "Java";
	String st2 = "Unix";

	Thread t1 = new Thread("My thread1") {
		public void run() {

			while (true) {
				synchronized (st) {
					synchronized (st2) {
						System.out.println(st + st2);
					}
				}
			}
		}
	};

	Thread t2 = new Thread() {
		public void run() {
			while (true) {
				synchronized (st2) {
					synchronized (st) {
						System.out.println(st2 + st);
					}
				}

			}
		}
	};

	public static void main(String st[]) {
		DeadLockTest d = new DeadLockTest();
		d.t1.start();
		d.t2.start();
	}
}
