class AnnoProcessor{
	public static void processAnnotations(Object obj){
		try {
			Class cls=obj.getClass();
			for(Field f:cls.getDeclaredFields()){
				Copyright i=f.getAnnotation(Copyright.class);
			}
		} catch (Exception e) {
			// TODO: handle exception
		}
	}
}